QRadioButton {
    /* 基础按钮样式 */
    border: 1px solid #ccc;
    border-radius: 4px;
    padding: 4px 8px;
    margin: 2px;
    
    /* 文本居中，类似普通按钮 */
    text-align: center;
    
    /* 隐藏默认指示器 */
    spacing: 3;
}


/* 自定义矩形指示器（核心修改） */
QRadioButton::indicator {
    width: 3px; /* 满足需求：宽度3 */
    height: 16px; /* 高度接近按钮（按钮总高24px - 上下各4px padding = 16px） */
    border-radius: 0; /* 取消圆角，确保是纯矩形 */
    background-color: #999; /* 未选中时的矩形颜色（浅灰） */
}

/* 选中状态的矩形样式（区分选中/未选中） */
QRadioButton::indicator:checked {
    background-color: #2196F3; /* 选中时的矩形颜色（示例：蓝色） */
    /* 可选：选中时加细边框，增强视觉区分 */
    /* border: 1px solid #1976D2; */
}

/* 未选中状态 - 灰暗底色（类似电灯关闭） */
QRadioButton:unchecked {
    background-color: #f0f0f0;
    color: #666;
    border-color: #ddd;
}

/* 选中状态 - 亮绿色（类似电灯开启） */
QRadioButton:checked {
    background-color: #7FFF00; /* 亮绿色 */
    color: #333;
    border-color: #66cc00;
}

/* 可选：另一种选中色（亮黄色） */
/* QRadioButton:checked {
    background-color: #FFFF99; 亮黄色
    color: #333;
    border-color: #ffcc66;
} */

/* 交互反馈 */
QRadioButton:hover:unchecked {
    background-color: #e8e8e8;
    border-color: #bbb;
}

QRadioButton:pressed {
    transform: scale(0.98);
}

/* 禁用状态 */
QRadioButton:disabled {
    background-color: #f5f5f5;
    color: #aaa;
    border-color: #eee;
}



